:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x562047b97df0 .scope module, "test" "test" 2 2;
 .timescale 0 0;
v0x562047b99830_0 .var "clk", 0 0;
v0x562047b9a1b0_0 .var "cnt", 31 0;
E_0x562047b98e20 .event posedge, v0x562047b99830_0;
    .scope S_0x562047b97df0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562047b99830_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x562047b9a1b0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x562047b97df0;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x562047b99830_0;
    %inv;
    %store/vec4 v0x562047b99830_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x562047b97df0;
T_2 ;
    %vpi_call 2 13 "$display", "init" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x562047b97df0;
T_3 ;
    %wait E_0x562047b98e20;
    %load/vec4 v0x562047b9a1b0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x562047b9a1b0_0, 0;
    %load/vec4 v0x562047b9a1b0_0;
    %cmpi/e 10000, 0, 32;
    %jmp/0xz  T_3.0, 4;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x562047b9a1b0_0, 0;
    %vpi_call 2 21 "$display", "cnt = ", v0x562047b9a1b0_0 {0 0 0};
T_3.0 ;
    %jmp T_3;
    .thread T_3;
